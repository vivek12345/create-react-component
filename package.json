{
  "name": "create-react-component",
  "displayName": "create-react-component",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createFunctionalReactComponent"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createFunctionalReactComponent",
        "title": "Create a functional react component"
      },
      {
        "command": "extension.createReactClassComponent",
        "title": "Create a class level react component"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.createFunctionalReactComponent",
          "group": "navigation"
        },
        {
          "command": "extension.createReactClassComponent",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^10.2.0",
    "prettier": "^1.18.2",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  }
}
